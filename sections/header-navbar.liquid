{% if template == 'index' %}
<div class="container container-full navbar-cont">
    <div class="navbar-left">
      <ul class="navbar-menu">
        {% for link in linklists['main-menu'].links %}
          <li class="navbar-menu-item txt-400 fs-14 cl-white">
            <a href="{{ link.url }}" id="nav-shop">{{ link.title }}</a>
          </li>
        {% endfor %}
      </ul>
      {% if localization.available_countries.size > 1 %}
        <localization-form>
          {% form 'localization' %}
            <div class="disclosure currency">
              <button type="button" class="disclosure__button currency-btn" aria-expanded="false" aria-controls="CountryList">
                <span class="fs-14">
                  {{ localization.country.currency.iso_code }}
                </span>
                <svg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M11.6199 5.2207L7.81655 9.02404C7.36738 9.4732 6.63238 9.4732 6.18322 9.02404L2.37988 5.2207" stroke="#ffffff" stroke-width="0.875" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
              </button>
      
              <ul id="CountryList" role="list" class="disclosure__list currency-list" hidden>
                {% for country in localization.available_countries %}
                  <li class="disclosure__item currency-list-item" tabindex="-1">
                    <a href="#"{% if country.iso_code == localization.country.iso_code %} aria-current="true"{% endif %} data-value="{{ country.iso_code }}">
                      {{ country.currency.iso_code }}
                    </a>
                  </li>
                {% endfor %}
              </ul>
      
              <input type="hidden" name="country_code" value="{{ localization.country.iso_code }}">
            </div>
          {% endform %}
        </localization-form>
      {% endif %}
    </div>
    <div class="navbar-center">
      <a href="{{ routes.root_url }}">
        <img src="https://cdn.shopify.com/s/files/1/0625/6263/1749/files/logo-white.png?v=1731811107" alt="" class="navbar-logo">
      </a>
    </div>
    <div class="navbar-right">
